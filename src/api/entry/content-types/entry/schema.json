{
  "kind": "collectionType",
  "collectionName": "entries",
  "info": {
    "singularName": "entry",
    "pluralName": "entries",
    "displayName": "Entry",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "CardText": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "Language": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Hungarian",
        "English",
        "Bi-Lingual"
      ]
    },
    "EntryType": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Blog",
        "Podcast",
        "Video"
      ],
      "required": true
    },
    "Profile": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Archival",
        "Academic",
        "Public"
      ],
      "required": true
    },
    "PodcastLink": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "Tags": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    },
    "Content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "contents.content-left",
        "contents.content",
        "contents.quote",
        "media.image",
        "media.video"
      ],
      "required": true
    },
    "Slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "Title"
    },
    "Author": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "AuthorStaff": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::staff-record.staff-record",
      "mappedBy": "Entry"
    },
    "EventType": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Exhibition",
        "Talk",
        "Movie Screening",
        "Theatre",
        "Music",
        "Conference",
        "Workshop",
        "University Teaching",
        "Research",
        "Scholarship",
        "Publication",
        "Internship",
        "Lecture",
        "Book Launch"
      ]
    },
    "OriginalCreationDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "rank": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    }
  }
}
