{
  "kind": "collectionType",
  "collectionName": "staff_records",
  "info": {
    "singularName": "staff-record",
    "pluralName": "staff-records",
    "displayName": "Staff",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Position": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "Bio": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks"
    },
    "Appearances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appearance.appearance",
      "mappedBy": "Staff"
    },
    "Slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "Name"
    },
    "Courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "Staff"
    },
    "Publications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::publication.publication",
      "mappedBy": "Staff"
    },
    "Email": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "Phone": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "FirstName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "LastName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "Unit": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Administration",
        "Archival Programs",
        "Audiovisual Unit",
        "Chief Archivist",
        "Director",
        "Event & Exhibition",
        "IT Office",
        "Library",
        "Public Awareness Unit",
        "Records Management",
        "Reference Services",
        "Research",
        "Verzio Filmfestival"
      ],
      "required": true
    },
    "Entries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::entry.entry",
      "inversedBy": "AuthorStaff"
    }
  }
}
