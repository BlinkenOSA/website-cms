{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news-item",
    "pluralName": "news-items",
    "displayName": "News",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "CardText": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "Profile": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Archivum",
        "Collections",
        "Academic",
        "Public"
      ],
      "required": true
    },
    "Tags": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    },
    "Content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "contents.content-left",
        "contents.content",
        "contents.quote",
        "contents.text-box",
        "media.image",
        "media.video",
        "contents.image-gallery",
        "components.divider-icon",
        "components.divider-line"
      ],
      "required": true
    },
    "Slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "Title"
    },
    "Author": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "AuthorStaff": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::staff-record.staff-record"
    },
    "ActivityType": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Academic News",
        "Announcement",
        "Archivum News",
        "Book Launch",
        "Collections News",
        "Community",
        "Conference",
        "Education Program",
        "Exhibition",
        "Film Screening",
        "Internship",
        "Job",
        "Join Us",
        "Lecture",
        "Library",
        "Music",
        "Partner Projects",
        "Performance",
        "Program Series",
        "Public Program News",
        "Publication",
        "Research",
        "Roundtable",
        "Scholarship",
        "Talk",
        "Theatre",
        "University Course",
        "Workshop"
      ]
    },
    "OriginalCreationDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "rank": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "default": 999
    }
  }
}
