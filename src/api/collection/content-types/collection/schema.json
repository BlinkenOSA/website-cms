      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks",
      "required": true
    },
    "Link": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "FrontPageSequence": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
