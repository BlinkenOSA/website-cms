{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "StartDate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date"
    },
    "EndDate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "date"
    },
    "Language": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "English",
        "Hungarian",
        "Bi-Lingual"
      ]
    },
    "HostType": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Online",
        "Hybrid",
        "In-Person"
      ]
    },
    "CardText": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "Content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks",
      "required": true
    },
    "Link": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "Tags": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    },
    "Slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "Title"
    },
    "Profiles": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": [
        "Academic",
        "Archival",
        "Partner",
        "Public"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "ButtonText": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    }
  }
}
